<Window x:Class="JetstyleTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JetstyleTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="MenuHoverHistoryList" ItemsSource="{Binding Path=Items}"></ListBox>
        <TextBlock x:Name="FocusAppName" FontSize="30"></TextBlock>
    </Grid>
</Window>
